generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model SalesData {
  id              Int      @id @default(autoincrement())
  date            DateTime
  revenue         Float
  productCategory category 
  region          String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}

enum category{
  Electronic
  Clothing
  Books
  Foods
  Toy
}